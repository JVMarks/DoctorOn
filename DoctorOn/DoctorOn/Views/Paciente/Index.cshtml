@model DoctorOn.Entidades.Paciente

<link href="~/Content/Calendar.css" rel="stylesheet" />

<div class="top">

    <section class="section_container">

        <section class="section_img">
        </section>

        <section class="section_perfil">
            <div class="section_direction">
                <h1>
                    @Html.DisplayFor(model => model.Nome_completo)
                </h1>
                <h3>
                  Nome do convênio:  @Html.DisplayFor(model => model.Nome_do_convenio, new { style = "font-weight: 600;" })
                </h3>
                <h3>
                    Sexo:  @Html.DisplayFor(model => model.Sexo, new { style = "font-weight: 600;" })
                </h3>
                <h3>
                    Data de nacismento:  @Html.DisplayFor(model => model.Datadenacimento, new { style = "font-weight: 600;" })
                </h3>
                <h3>
                   Numero de telefone: @Html.DisplayFor(model => model.Telefone, new { style = "font-weight: 600;" })
                </h3>
                <h3>
                   Endereço:  @Html.DisplayFor(model => model.Endereco, new { style = "font-weight: 600;" })
                </h3>

            </div>
        </section>

    </section>

</div>